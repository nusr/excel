  },
  "dependencies": {
    "excel-collab": "*",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "uuid": "^12.0.0",
    "y-websocket": "^3.0.0",
    "yjs": "^13.6.27"
  }
}
