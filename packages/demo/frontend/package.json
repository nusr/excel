  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@sentry/react": "^10.32.1"
  },
  "dependencies": {
    "excel-collab": "*",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "uuid": "^13.0.0",
    "y-websocket": "^3.0.0",
    "yjs": "^13.6.28"
  }
}
