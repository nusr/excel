{
  "name": "excel-collab",
  "version": "0.1.6",
  "description": "Online Collaboration Excel",
  "author": "Steve Xu <stevexugc@gmail.com>",
  "bugs": "https://github.com/nusr/excel/issues",
  "homepage": "https://github.com/nusr/excel",
  "repository": "git+https://github.com/nusr/excel.git",
  "license": "MIT",
  "module": "./lib/index.es.js",
  "main": "./lib/index.umd.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.es.js",
      "module": "./lib/index.es.js",
      "require": "./lib/index.umd.js"
    },
    "./style.css": "./lib/style.css",
    "./package.json": "./package.json"
  },
  "scripts": {
    "type-check": "tsc",
    "build": "vite build",
    "test": "jest --forceExit --detectOpenHandles",
    "test:ci": "JEST_JUNIT_CLASSNAME=\"{filepath}\" jest --reporters=jest-junit --forceExit --detectOpenHandles"
  },
  "dependencies": {
    "chart.js": "^4.5.1",
    "comlink": "^4.4.2",
    "jszip": "^3.10.1",
    "numfmt": "^3.2.3",
    "uuid": "^13.0.0",
    "yjs": "^13.6.27",
    "zustand": "^5.0.8"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "keywords": [
    "react",
    "yjs",
    "collaboration",
    "excel",
    "xlsx",
    "spreadsheet"
  ],
  "files": [
    "lib",
    "CHANGELOG.md",
    "README.md",
    "package.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "y-protocols": "^1.0.6"
  }
}
