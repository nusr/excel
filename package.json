{
  "name": "excel",
  "version": "1.0.0",
  "description": "web excel",
  "main": "lib/excel.umd.js",
  "module": "lib/excel.esm.js",
  "scripts": {
    "start": "npm run build -- --nodeEnv development",
    "build": "node ./scripts/esbuild.js",
    "test": "jest --config ./scripts/unit-test-jest.config.js --forceExit --detectOpenHandles",
    "coverage": "npm run test -- --coverage",
    "ci": "npm run e2e && npm run test && npm run build && npm run type-check && npm run update-md && npm run lint",
    "type-check": "npx tsc",
    "postinstall": "node ./post-install.js",
    "prebuild": "npm run postinstall",
    "e2e": "jest --config ./scripts/e2e-jest.config.js --forceExit --detectOpenHandles",
    "lint": "npx oxlint@latest && eslint --fix --quiet --ext  .ts,.tsx ./",
    "update-md": "node ./scripts/update-md.js",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nusr/excel.git"
  },
  "keywords": [],
  "author": "Steve Xu",
  "license": "MIT",
  "homepage": "https://nusr.github.io/excel",
  "devDependencies": {
    "@testing-library/react": "14.2.1",
    "@types/jest": "29.5.12",
    "@types/react": "18.2.63",
    "@types/react-dom": "18.2.20",
    "@typescript-eslint/eslint-plugin": "7.1.1",
    "@typescript-eslint/parser": "7.1.1",
    "canvas": "2.11.2",
    "esbuild": "0.20.1",
    "eslint": "8.57.0",
    "husky": "^9.0.11",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "puppeteer": "22.4.0",
    "typescript": "5.3.3"
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "@sentry/react": "^7.105.0",
    "chart.js": "4.4.2",
    "jszip": "3.10.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "yjs": "13.6.14"
  },
  "pnpm": {
    "overrides": {
      "ip": "2.x"
    }
  }
}
