{
  "name": "excel",
  "version": "1.0.0",
  "description": "web excel",
  "main": "dist/index.js",
  "scripts": {
    "start": "set NODE_ENV=development && npm run build",
    "build": "node ./build/build.js",
    "serve": "node ./build/server.js",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "test": "npx jest --config ./unittest/jest.config.js",
    "postinstall": "npm run check-version",
    "test-coverage": "npm run test -- --coverage",
    "test-watch": "npm run test -- --watch",
    "check-version": "ncu -u",
    "ci": "npm run lint && npm run test && npm run build",
    "deploy": "./scripts/deploy.sh",
    "typecheck": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/nusr/excel.git"
  },
  "keywords": [],
  "author": "Steve Xu <1161176156@qq.com>",
  "license": "MIT",
  "homepage": "https://nusr.github.io/excel",
  "dependencies": {
    "immer": "^8.0.1",
    "lodash-es": "^4.17.20",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "styled-components": "^5.2.1",
    "use-immer": "^0.4.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/lodash-es": "^4.17.4",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "esbuild": "^0.8.34",
    "esbuild-jest": "file:./esbuild-jest",
    "eslint": "^7.18.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3",
    "svg-parser": "^2.0.4",
    "typescript": "^4.1.3"
  },
  "browser": {
    "path": "path-browserify"
  }
}
