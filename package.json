{
  "name": "excel",
  "version": "1.0.0",
  "description": "web excel",
  "main": "dist/index.js",
  "scripts": {
    "start": "set NODE_ENV=development && npm run build",
    "build": "node ./scripts/build.js",
    "serve": "node ./scripts/server.js",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "typecheck": "tsc",
    "test": "npx jest --config ./unittest/jest.config.js",
    "postinstall": "npm run check-version && npm run dependencies",
    "reinstall": "node ./scripts/delete.js && npm i",
    "test:coverage": "npm run test -- --coverage",
    "dependencies": "node ./scripts/dependencies.js",
    "check-version": "ncu -u"
  },
  "pre-commit": [
    "lint",
    "test",
    "build"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/nusr/excel.git"
  },
  "keywords": [],
  "author": "Steve Xu <1161176156@qq.com>",
  "license": "MIT",
  "homepage": "https://github.com/nusr/excel",
  "dependencies": {
    "immer": "^8.0.0",
    "lodash-es": "^4.17.15",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "styled-components": "^5.2.1",
    "use-immer": "^0.4.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.19",
    "@types/lodash-es": "^4.17.4",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "esbuild": "^0.8.24",
    "eslint": "^7.16.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3",
    "pre-commit": "^1.2.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "browser": {
    "path": "path-browserify"
  }
}
