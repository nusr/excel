{
  "name": "excel",
  "version": "1.0.0",
  "description": "web excel",
  "main": "lib/excel.umd.js",
  "module": "lib/excel.esm.js",
  "scripts": {
    "start": "npm run build -- --nodeEnv development",
    "build": "node ./scripts/esbuild.js",
    "test": "jest --config ./scripts/unit-test-jest.config.js --forceExit --detectOpenHandles",
    "test-only": "jest ./src/__tests__/FloatElement.test.tsx --config ./scripts/unit-test-jest.config.js --forceExit --detectOpenHandles --verbose",
    "coverage": "npm run test -- --coverage",
    "ci": "npm run e2e && npm run test && npm run build && npm run update-md && npm run lint && npx tsc",
    "postinstall": "node ./post-install.js",
    "prebuild": "npm run postinstall",
    "e2e": "npx playwright test",
    "lint": "npx oxlint@latest --fix",
    "update-md": "node ./scripts/update-md.js",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nusr/excel.git"
  },
  "keywords": [],
  "author": "Steve Xu",
  "license": "MIT",
  "homepage": "https://nusr.github.io/excel",
  "devDependencies": {
    "@playwright/test": "1.42.1",
    "@testing-library/jest-dom": "6.4.2",
    "@testing-library/react": "14.2.2",
    "@types/jest": "29.5.12",
    "@types/node": "20.12.2",
    "@types/react": "18.2.74",
    "@types/react-dom": "18.2.23",
    "canvas": "2.11.2",
    "esbuild": "0.20.2",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "typescript": "5.4.3"
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "@sentry/react": "7.109.0",
    "chart.js": "4.4.2",
    "jszip": "3.10.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "packageManager": "pnpm@8.7.4+sha1.9dfcac89c4010a00ce092c3cda76dab72cd99198"
}
