{
  "name": "excel",
  "version": "1.0.0",
  "description": "web excel",
  "main": "lib/excel.umd.js",
  "module": "lib/excel.esm.js",
  "scripts": {
    "start": "npm run build -- NODE_ENV=development",
    "build": "node ./scripts/esbuild.js",
    "test": "jest --config ./scripts/unit-test-jest.config.js --forceExit --detectOpenHandles",
    "coverage": "npm run test -- --coverage",
    "ci": "npm run test && npm run type-check && npm run build && npm run e2e && npm run lint",
    "type-check": "npx tsc",
    "postinstall": "node ./post-install.js",
    "prebuild": "node ./post-install.js",
    "e2e": "jest --config ./scripts/e2e-jest.config.js --forceExit --detectOpenHandles",
    "lint": "eslint --fix --quiet --ext  .ts,.tsx ./"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nusr/excel.git"
  },
  "keywords": [],
  "author": "Steve Xu",
  "license": "MIT",
  "homepage": "https://nusr.github.io/excel",
  "devDependencies": {
    "@testing-library/react": "14.1.2",
    "@types/jest": "29.5.11",
    "@types/react": "18.2.45",
    "@types/react-dom": "18.2.17",
    "@typescript-eslint/eslint-plugin": "6.14.0",
    "@typescript-eslint/parser": "6.14.0",
    "canvas": "2.11.2",
    "esbuild": "0.19.9",
    "eslint": "8.55.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "puppeteer": "21.6.0",
    "typescript": "5.3.3"
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "jszip": "3.10.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  }
