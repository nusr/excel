{
  "name": "excel",
  "version": "1.0.0",
  "description": "web excel",
  "main": "lib/excel.umd.js",
  "module": "lib/excel.esm.js",
  "scripts": {
    "start": "node ./scripts/esbuild.js",
    "build": "npm run start -- --nodeEnv production",
    "test": "jest --config ./jest.config.js --forceExit --detectOpenHandles",
    "coverage": "npm run test -- --coverage",
    "ci": "npm run e2e && npm run test && npm run build && npm run update-md && npm run lint && npx tsc && npm run repeat",
    "e2e": "npx playwright test",
    "lint": "npx oxlint@latest --fix",
    "update-md": "node ./scripts/update-md.js",
    "prepare": "husky",
    "repeat": "jscpd --ignore \"**/*.test.tsx,**/*.test.ts\" ./src -r html -m strict -l 7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nusr/excel.git"
  },
  "keywords": [],
  "author": "Steve Xu",
  "license": "MIT",
  "homepage": "https://nusr.github.io/excel",
  "devDependencies": {
    "@playwright/test": "1.43.1",
    "@testing-library/jest-dom": "6.4.5",
    "@testing-library/react": "15.0.6",
    "@types/jest": "29.5.12",
    "@types/node": "20.12.8",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.0",
    "canvas": "2.11.2",
    "esbuild": "0.20.2",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "ts-jest": "29.1.2",
    "typescript": "5.4.5"
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "@sentry/react": "7.113.0",
    "chart.js": "4.4.2",
    "jszip": "3.10.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "ssf": "0.11.2"
  }
}
