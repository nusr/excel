{
  "name": "excel",
  "version": "1.0.0",
  "description": "web excel",
  "main": "lib/excel.umd.js",
  "module": "lib/excel.esm.js",
  "scripts": {
    "start": "npm run build -- NODE_ENV=development",
    "build": "node ./scripts/esbuild.js",
    "test": "jest --config ./scripts/unit-test-jest.config.js",
    "coverage": "npm run test -- --coverage",
    "ci": "npm run test && npm run type-check && npm run build && npm run e2e",
    "type-check": "npx tsc",
    "postinstall": "node ./post-install.js",
    "prebuild": "node ./post-install.js",
    "e2e": "concurrently --kill-others --success last \"npm:start\" \"npm:e2e-test\"",
    "e2e-test": "jest --config ./scripts/e2e-jest.config.js --forceExit --detectOpenHandles"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nusr/excel.git"
  },
  "keywords": [],
  "author": "Steve Xu",
  "license": "MIT",
  "homepage": "https://nusr.github.io/excel",
  "devDependencies": {
    "@testing-library/react": "14.0.0",
    "@types/jest": "29.5.5",
    "@types/react": "18.2.28",
    "@types/react-dom": "18.2.13",
    "canvas": "2.11.2",
    "concurrently": "8.2.1",
    "esbuild": "0.19.4",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "puppeteer": "21.3.8",
    "typescript": "5.2.2"
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "jszip": "^3.10.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  }
}
