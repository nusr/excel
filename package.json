{
  "name": "excel",
  "version": "1.0.0",
  "description": "web excel",
  "main": "lib/excel.umd.js",
  "module": "lib/excel.esm.js",
  "scripts": {
    "start": "node ./scripts/esbuild.js",
    "build": "npm run start -- --nodeEnv production",
    "test": "jest --config ./jest.config.js --forceExit --detectOpenHandles",
    "coverage": "npm run test -- --coverage",
    "ci": "npm run repeat && npm run e2e && npm run test && npm run build && npm run update-md && npm run lint && npx tsc",
    "e2e": "npx playwright test",
    "lint": "npx oxlint@latest --fix",
    "update-md": "node ./scripts/update-md.js",
    "prepare": "husky && npx playwright install --with-deps",
    "repeat": "jscpd --ignore \"**/*.test.tsx,**/*.test.ts\" -r html -m strict -l 7 -b -g --exitCode 1 .",
    "typedoc": "npx typedoc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nusr/excel.git"
  },
  "keywords": [],
  "author": "Steve Xu",
  "license": "MIT",
  "homepage": "https://nusr.github.io/excel",
  "devDependencies": {
    "@playwright/test": "1.44.1",
    "@testing-library/jest-dom": "6.4.5",
    "@testing-library/react": "15.0.7",
    "@types/jest": "29.5.12",
    "@types/node": "20.12.12",
    "@types/pixelmatch": "5.2.6",
    "@types/pngjs": "6.0.5",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "canvas": "2.11.2",
    "esbuild": "0.21.4",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "pixelmatch": "5.3.0",
    "pngjs": "7.0.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "ts-jest": "29.1.3",
    "typedoc": "0.25.13",
    "typescript": "5.4.5"
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "chart.js": "4.4.3",
    "jszip": "3.10.1",
    "ssf": "https://cdn.sheetjs.com/ssf-0.11.3/ssf-0.11.3.tgz"
  },
  "peerDependencies": {
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "optionalDependencies": {
    "@sentry/react": "8.4.0"
  }
}