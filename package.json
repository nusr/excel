{
  "name": "excel",
  "version": "1.0.0",
  "description": "web excel",
  "main": "lib/excel.umd.js",
  "module": "lib/excel.esm.js",
  "scripts": {
    "start": "node ./scripts/open.js dist/index.html && npm run build -- NODE_ENV=development",
    "build": "node ./scripts/esbuild.js",
    "test": "jest --config ./scripts/unit-test-jest.config.js",
    "coverage": "npm run test -- --coverage",
    "ci": "npm run build && npm run test && npm run type-check && npm run e2e",
    "type-check": "npx tsc",
    "postinstall": "node ./post-install.js",
    "prebuild": "node ./post-install.js",
    "e2e": "jest --config ./scripts/e2e-jest.config.js --forceExit --detectOpenHandles"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nusr/excel.git"
  },
  "keywords": [],
  "author": "Steve Xu",
  "license": "MIT",
  "homepage": "https://nusr.github.io/excel",
  "devDependencies": {
    "@types/jest": "29.2.5",
    "esbuild": "0.17.0",
    "jest": "29.3.1",
    "jest-environment-jsdom": "29.3.1",
    "puppeteer": "18.2.1",
    "typescript": "4.9.4"
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {}
}
