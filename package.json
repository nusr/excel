{
  "name": "excel",
  "version": "1.0.0",
  "description": "web excel",
  "main": "dist/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development npm run build",
    "build": "node ./build/build.js",
    "serve": "node ./build/server.js",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "test": "npx jest --config ./unittest/jest.config.js",
    "test-coverage": "npm run test -- --coverage",
    "test-watch": "npm run test -- --watch",
    "check-version": "ncu -u",
    "ci": "npm run build && npm run lint && npm run test",
    "deploy": "deploy.sh",
    "typecheck": "tsc",
    "postinstall": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/nusr/excel.git"
  },
  "keywords": [],
  "author": "Steve Xu <1161176156@qq.com>",
  "license": "MIT",
  "homepage": "https://nusr.github.io/excel",
  "dependencies": {
    "immer": "8.0.1",
    "lodash": "4.17.21",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "styled-components": "5.2.1"
  },
  "devDependencies": {
    "@types/jest": "26.0.20",
    "@types/lodash": "4.14.168",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.1",
    "@types/styled-components": "5.1.7",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "cross-env": "7.0.3",
    "esbuild": "0.8.52",
    "eslint": "7.21.0",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "5.1.3",
    "jest": "26.6.3",
    "svg-parser": "2.0.4",
    "typescript": "4.2.3"
  },
  "browser": {
    "path": "path-browserify"
  }
}
