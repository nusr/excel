{
  "name": "excel",
  "version": "1.0.0",
  "description": "web excel",
  "main": "dist/index.js",
  "scripts": {
    "start": "node ./build/build.js NODE_ENV=development",
    "build": "node ./build/build.js NODE_ENV=production",
    "test": "jest --config ./unittest/jest.config.js",
    "coverage": "npm run test -- --coverage",
    "watch": "npm run test -- --watch",
    "ci": "npm run build && npm run test && npm run type-check && npm run e2e",
    "deploy": "deploy.sh",
    "type-check": "node scripts/rm.js && tsc",
    "postinstall": "husky install",
    "update": "npx ncu -u && npm i && npm run ci",
    "pree2e": "npm run build",
    "e2e": "jest --config ./e2e/lib/jest.config.js --forceExit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nusr/excel.git"
  },
  "keywords": [],
  "author": "Steve Xu <1161176156@qq.com>",
  "license": "MIT",
  "homepage": "https://nusr.github.io/excel",
  "dependencies": {
    "immer": "9.0.12",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/jest": "27.4.1",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.11",
    "esbuild": "0.14.23",
    "husky": "^7.0.4",
    "istanbul-lib-coverage": "^3.2.0",
    "jest": "27.5.1",
    "puppeteer": "^13.4.0",
    "typescript": "4.5.5"
  },
  "browser": {
    "path": "path-browserify"
  },
  "engines": {
    "node": ">=12.10.0"
  }
}
