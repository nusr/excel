{
  "name": "excel",
  "version": "1.0.0",
  "description": "web excel",
  "main": "dist/index.js",
  "scripts": {
    "start": "node ./build/build.js NODE_ENV=development",
    "build": "node ./build/build.js NODE_ENV=production",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "test": "npx jest --config ./unittest/jest.config.js",
    "coverage": "npm run test -- --coverage",
    "watch": "npm run test -- --watch",
    "ci": "npm run build && npm run lint && npm run test && npm run typecheck",
    "deploy": "deploy.sh",
    "typecheck": "node scripts/rm.js && tsc",
    "postinstall": "node ./scripts/link-hook.js",
    "update": "ncu -u"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nusr/excel.git"
  },
  "keywords": [],
  "author": "Steve Xu <1161176156@qq.com>",
  "license": "MIT",
  "homepage": "https://nusr.github.io/excel",
  "dependencies": {
    "immer": "9.0.2",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/jest": "26.0.23",
    "@types/react": "17.0.8",
    "@types/react-dom": "17.0.5",
    "@typescript-eslint/eslint-plugin": "4.25.0",
    "@typescript-eslint/parser": "4.25.0",
    "esbuild": "0.12.5",
    "eslint": "7.27.0",
    "eslint-plugin-import": "2.23.3",
    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "jest": "27.0.1",
    "svg-parser": "2.0.4",
    "typescript": "4.3.2"
  },
  "browser": {
    "path": "path-browserify"
  },
  "engines": {
    "node": ">=12.10.0"
  }
