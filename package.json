{
  "name": "excel",
  "version": "1.0.0",
  "description": "web excel",
  "main": "dist/index.js",
  "scripts": {
    "start": "node ./build/build.js NODE_ENV=development",
    "build": "node ./build/build.js NODE_ENV=production",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint-fix": "npm run lint -- --fix",
    "test": "jest --config ./unittest/jest.config.js --detectOpenHandles",
    "coverage": "npm run test -- --coverage",
    "watch": "npm run test -- --watch",
    "ci": "npm run build && npm run lint && npm run test && npm run type-check",
    "deploy": "deploy.sh",
    "type-check": "node scripts/rm.js && tsc",
    "postinstall": "husky install",
    "update": "ncu -u && npm i",
    "e2e-test": "jest --config ./e2e/lib/jest.config.js --detectOpenHandles --forceExit",
    "e2e": "concurrently \"node ./build/build.js NODE_ENV=development CI=true\" \"npm run e2e-test\" -k"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nusr/excel.git"
  },
  "keywords": [],
  "author": "Steve Xu <1161176156@qq.com>",
  "license": "MIT",
  "homepage": "https://nusr.github.io/excel",
  "dependencies": {
    "immer": "9.0.7",
    "jszip": "3.7.1",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@types/jest": "27.0.3",
    "@types/react": "17.0.37",
    "@types/react-dom": "17.0.11",
    "@typescript-eslint/eslint-plugin": "5.5.0",
    "@typescript-eslint/parser": "5.5.0",
    "babel-plugin-istanbul": "^6.1.1",
    "concurrently": "^7.0.0",
    "esbuild": "0.14.2",
    "eslint": "8.4.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-react": "7.27.1",
    "eslint-plugin-react-hooks": "4.3.0",
    "husky": "^7.0.4",
    "jest": "27.4.3",
    "puppeteer": "^13.3.2",
    "svg-parser": "2.0.4",
    "typescript": "4.5.2"
  },
  "browser": {
    "path": "path-browserify"
  },
  "engines": {
    "node": ">=12.10.0"
  }
}
