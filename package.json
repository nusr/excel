{
  "name": "excel",
  "version": "1.0.0",
  "description": "web excel",
  "main": "dist/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development node ./build/build.js",
    "build": "cross-env NODE_ENV=production  node ./build/build.js",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "test": "npx jest --config ./unittest/jest.config.js",
    "test-coverage": "npm run test -- --coverage",
    "test-watch": "npm run test -- --watch",
    "ci": "npm run build && npm run lint && npm run test && npm run typecheck",
    "deploy": "deploy.sh",
    "typecheck": "node scripts/rm.js && tsc",
    "postinstall": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nusr/excel.git"
  },
  "keywords": [],
  "author": "Steve Xu <1161176156@qq.com>",
  "license": "MIT",
  "homepage": "https://nusr.github.io/excel",
  "dependencies": {
    "immer": "9.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "styled-components": "5.2.1"
  },
  "devDependencies": {
    "@types/jest": "26.0.22",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.3",
    "@types/styled-components": "5.1.9",
    "@typescript-eslint/eslint-plugin": "4.19.0",
    "@typescript-eslint/parser": "4.19.0",
    "cross-env": "7.0.3",
    "esbuild": "0.10.2",
    "eslint": "7.23.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-react": "7.23.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "5.2.0",
    "jest": "26.6.3",
    "svg-parser": "2.0.4",
    "typescript": "4.2.3"
  },
  "browser": {
    "path": "path-browserify"
  },
  "engines": {
    "node": ">=12.10.0"
  }
